{
  "name": "@stencil/storybook-plugin",
  "version": "0.5.9",
  "description": "Storybook plugin for Stencil",
  "license": "MIT",
  "homepage": "https://github.com/stenciljs/storybook",
  "bugs": {
    "url": "https://github.com/stenciljs/storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/storybook.git",
    "directory": "packages/plugin"
  },
  "keywords": [
    "storybook",
    "stencil",
    "stenciljs",
    "webcomponents",
    "components",
    "framework"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "template",
    "@types",
    "preset.js"
  ],
  "//": "not sure why dist/preview is needed, but it is",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js",
      "require": "./dist/preset.cjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./dist/preview.js": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "watch": "tsdown --watch",
    "build": "tsdown"
  },
  "dependencies": {
    "@storybook/builder-vite": "^9.0.8 || ^10.0.0",
    "@storybook/global": "^5.0.0",
    "@storybook/html": "^9.0.8 || ^10.0.0",
    "preact-render-to-string": "^6.6.4",
    "react-docgen-typescript": "^2.4.0",
    "unplugin-stencil": "^0.4.1"
  },
  "peerDependencies": {
    "@stencil/core": "^4.30.0",
    "storybook": "^9.0.8 || ^10.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.39.0",
    "@types/node": "^22.19.2",
    "tsdown": "^0.17.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.7"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
